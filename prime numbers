#include<stdio.h>
void main()
     { 
      int i,prime,upper,lower,n;
      printf("Enter the lower limit");
      scanf("%d",&lower);
      printf("\n Enter the upper limit");
      scanf("%d",&upper);
      printf("\n Prime numbers list is ");
      for (i=2;i<n/2;i++)
         if(n%1==0)
           {
            prime=0;
            break;
           }
          
          if(prime)
            {
            printf('\t %d",n);
            }
       }
            
      
