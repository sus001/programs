#include<stdio.h>
int main()
   {
    float,principle,time,rate,SI;
    /* Input principle ,rate and time */
    printf("Enter principe(amount):");
    scanf("%f",&principle);
    printf("Enter time:");
    scanf("%f",&time);
    printf("Enter rate:");
    scanf("%f",&rate);
    /* Calculate Simple Interest */
    SI=(principle*time*rate)/100;
    /* Print the resultant value of SI */
    printf("Simple interest =%f",SI);
    return 0;
    }
